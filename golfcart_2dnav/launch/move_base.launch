<launch>
	<master auto="start"/>
	
	<!--node name="map_server" pkg="map_server" type="map_server" args="$(find golfcart_map)/map.pgm resolution"/-->
	
	<!-- run AMCL -->
	<include file="$(find amcl)/examples/amcl_omni.launch" />
	
	<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
	  <rosparam file="$(find golfcart_2dnav)/config/costmap_common_params.yaml" command="load" ns="global_costmap"/>
	  <rosparam file="$(find golfcart_2dnav)/config/costmap_common_params.yaml" command="load" ns="local_costmap"/>
	  <rosparam file="$(find golfcart_2dnav)/config/local_costmap_params.yaml" command="load"/>
	  <rosparam file="$(find golfcart_2dnav)/config/global_costmap_params.yaml" command="load"/>
	  <rosparam file="$(find goldcart_2dnav)/config/base_local_planner_params.yaml" command="load"/>
	</node>
</launch>
